from time import *
# 1  数字排列
print("第一题")
num=0
for a in range(1,5):
   for b in range(1,5):
       for c in range(1,5):
           if(a!=b)and(a!=c)and(b!=c):
             print(a,b,c)
             num+=1
print(num)
print()



# 2 比较最大值
print("第二题")
n=10
m=39
k=78
max=n if n>m else m
max=max if max>k else k
print("max=",max)
t=n if n>m and a>k else m if m>k else k
print("t=",t)
print()


# 3求100以内奇数和
print("第三题")
a=0
i=1
while i<100 :
    a=a+i
    i+=2
print(a)
print()


# 4       100以内7的倍数之和
print("第四题")
b=0
j=0
while j<100 :
    j+=1
    if j%7==0 :
        b=b+j
print(b)
print()



# 5   水仙花数
print("第五题")
q=100
while q<1000 :
    w=q//100
    e=(q//10)%10
    r=q%10
    if q==w**3+e**3+r**3 :
        print(q)
    q+=1
print()



